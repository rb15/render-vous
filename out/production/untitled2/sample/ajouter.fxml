<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


      <VBox prefHeight="300.0" prefWidth="325.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ajouterController">
         <children>
            <HBox prefHeight="245.0" prefWidth="307.0">
               <children>
                  <VBox prefHeight="162.0" prefWidth="61.0" spacing="29.0">
                     <children>
                        <Label alignment="CENTER" text="Nom" />
                        <Label alignment="CENTER" layoutX="10.0" layoutY="10.0" text="Prenom" />
                        <Label alignment="CENTER" layoutX="10.0" layoutY="10.0" text="Date" />
                        <Label alignment="CENTER" layoutX="10.0" layoutY="47.0" text="Ville" />
                        <Label alignment="CENTER" layoutX="10.0" layoutY="148.0" text="Temp" />
                     </children>
                  </VBox>
                  <VBox prefHeight="205.0" prefWidth="176.0" spacing="20.0">
                     <children>
                        <TextField fx:id="nom_a" />
                        <TextField fx:id="prenom_a" />
                        <DatePicker fx:id="date_a" layoutX="10.0" layoutY="35.0" />
                        <TextField fx:id="ville_a" layoutX="10.0" layoutY="10.0" />
                        <TextField fx:id="temp_a" layoutX="10.0" layoutY="10.0" maxWidth="50.0" prefHeight="25.0" prefWidth="50.0" />
                     </children>
                     <padding>
                        <Insets left="20.0" />
                     </padding>
                  </VBox>
               </children>
               <padding>
                  <Insets bottom="20.0" left="50.0" right="20.0" top="20.0" />
               </padding>
            </HBox>
            <HBox prefHeight="37.0" prefWidth="200.0" spacing="30.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#ajout" text="Ajouter" />
                  <Button mnemonicParsing="false" onAction="#annu" text="Annuler" />
               </children>
               <padding>
                  <Insets bottom="20.0" left="90.0" right="20.0" top="10.0" />
               </padding>
            </HBox>
         </children>
      </VBox>

